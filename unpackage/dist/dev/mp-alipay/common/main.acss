.tabs1 {
  background: #F5F5F5;
  padding: 0.25rem;
  background: #fff;
}
.tabs1 > .title1 {
  width: 100%;
  height: 10rem;
}
.tabs1 > .title1 .swiper-box {
  height: 10rem;
}
.tabs1 > .title2 {
  padding: 0.5rem;
}
.tabs1 > .title2 > uni-navigator {
  height: 3rem;
  width: 7rem;
}
.tabs1 > .title2 > uni-navigator > uni-button {
  display: inline-block;
  line-height: 1rem;
  font-size: 0.7rem;
  padding: 0.5rem;
}
.tabs1 > .title2 > uni-navigator > uni-button > .bigsize {
  font-size: 1rem;
}
.tabs1 > .title2 > uni-navigator > uni-button > .minsize {
  font-size: 0.6rem;
}
.tabs1 > .title3 .uni-searchbar {
  height: 1.6rem;
}
.tabs1 > .title3 .uni-searchbar .uni-searchbar__box {
  height: 1.6rem;
  line-height: 1.6rem;
  border-radius: 1rem !important;
}
.tabs1 > .title4 .segmented-control {
  height: 1.8rem;
}
.tabs1 > .title4 .list {
  padding: 0.5rem;
  background: #F5F5F5;
}
.tabs1 > .title4 .list > uni-view {
  margin-bottom: 0.5rem;
  background: #fff;
  border-radius: 0.5rem;
}
.tabs1 > .title4 .list .min-size {
  font-size: 0.7rem;
  color: #9B9B9B;
}
.tabs1 > .title4 .list .min-size .red {
  color: #F55955;
}
.tabs1 > .title4 .list .min-size .big {
  font-size: 0.9rem;
}
.tabs1 > .title4 .list .min-size .line {
  text-decoration: line-through;
}
.tabs1 > .title4 .list .text-s {
  padding-bottom: 0.25rem;
}
.tabs1 > .title4 .list .text-s > uni-text {
  line-height: 1.5rem;
  height: 1.5rem;
}
.tabs1 > .title4 .list .text-s > .button {
  float: right;
  margin-right: 0.5rem;
  display: inline-block;
  line-height: 1.5rem;
  height: 1.5rem;
  font-size: 0.7rem;
  padding: 0 0.5em;
}

